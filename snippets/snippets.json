{
	"If mod": {
		"prefix": ["ifmod", "_if_mod"],
		"body": [
			"_if_mod(${mod})",
			"    ${Content}",
			"_if_end()"
		],
		"description": "If mod"
	},
	"If elif mode": {
		"prefix": ["ifelmod"],
		"body": [
			"_if_mod(${mod1})",
			"    ${Content}",
			"_elif_mod(${mod2})",
			"    ${Content2}",
			"_if_end()"
		],
		"description": "if elif mod"
	},
	"elif mod": {
		"prefix": ["elifmod", "_elif_mod"],
		"body": "_elif_mod(${mod})",
